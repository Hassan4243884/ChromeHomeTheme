{
  "manifest_version": 3,
  "name": "HomeTheme - Custom New Tab",
  "version": "1.0.0",
  "description": "A beautiful custom new tab page with nature backgrounds, clock, weather, and search",
  "permissions": ["storage", "geolocation", "tabs", "windows"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
